<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Visualizar Cliente</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="row">
  <p>
    <strong>Nome:</strong>
    <%= @cliente.nome %>
  </p>

  <p>
    <strong>Cpf cnpj:</strong>
    <%= @cliente.cpf_cnpj %>
  </p>

  <p>
    <strong>Data nascimento:</strong>
    <%= @cliente.data_nascimento %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= @cliente.email %>
  </p>

  <%= link_to 'Edit', edit_cliente_path(@cliente) %> |
  <%= link_to 'Back', clientes_path %>
</div>
<!-- /.row -->
